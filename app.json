{
  "expo": {
    "homepage": "https://github.com/frezva2/TroFii",
    "name": "TroFii",
    "description": "Looking for best food offers, food picker app, great food service & amazing dining experience around you? Scroll through to find best foods to order!",
    "slug": "TroFii",
    "owner": "frezva2",
    "currentFullName": "",
    "originalFullName": "",
    "privacy": "public",
    "scheme": "trofii",
    "userInterfaceStyle": "automatic",
    "platforms": ["ios", "android"],
    "version": "2.9.1",
    "orientation": "portrait",
    "facebookAppId": "",
    "facebookAutoInitEnabled": true,
    "facebookDisplayName": "TroFii",
    "facebookScheme": "",
    "assetBundlePatterns": [
      "./assets/Trofii-Splash.gif",
      "./assets/splash_2.png",
      "./assets/signinbkg.png",
      "./assets/header3.png",
      "./assets/NoNotifications.png",
      "./assets/Egift.png",
      "./assets/faverest.png",
      "./assets/NoPictureYet.png",
      "./assets/fonts/Montserrat-Medium.ttf",
      "./assets/fonts/Montserrat-Bold.ttf",
      "./assets/fonts/Montserrat-SemiBold.ttf",
      "./assets/fonts/Montserrat-Regular.ttf",
      "./assets/fonts/BerkshireSwash-Regular.ttf",
      "./assets/fonts/Poppins-Medium.ttf"
    ],
    "icon": "./assets/Icon_App_IOS2.png",
    "splash": {
      "image": "./assets/splash_2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "TroFii",
      "icon": "./assets/Icon_App_IOS2.png",
      "requireFullScreen": true,
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Your current location will be displayed on the map and used to search for restaurants or foods near you.",
        "NSPhotoLibraryUsageDescription": "The photo you upload from your library will be set as your profile picture.",
        "NSUserTrackingUsageDescription": "Your Name and Email address will be use to avoid froud and duplication account also it will help us to know who upload which picture in case of volition and froud. TroFii will NOT sell your data to any third party nor use in any AD.",
        "NSCameraUsageDescription": "The photo you upload using your camera will be set as your profile picture."
      },
      "config": {
        "googleMapsApiKey": ""
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.www.worests",
      "googleServicesFile": "./google-services.json",
      "versionCode": 191,
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#C90611"
      },
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "web": {
      "config": {
        "firebase": {
          "appId": "",
          "apiKey": "",
          "clientId": "",
          "authDomain": "",
          "databaseURL": "",
          "projectId": "",
          "storageBucket": "",
          "messagingSenderId": "",
          "measurementId": ""
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": ""
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "Your Name and Email address will be use to avoid froud and duplication account also it will help us to know who upload which picture in case of volition and froud. TroFii will NOT sell your data to any third party nor use in any AD."
        }
      ],
      "expo-font"
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": ""
    }
  }
}
